
public class IMath {
	public static int j=0;
	public static int math(int z) {
		j++;
		int a = 1, b=1;
		if (z==0 || z==1) {
			return z;
		}
		for (int i=2; i<z;i++) {
			int c=a+b;
			a=b;
			b=c;
		}
			return a;	
	}
	public static void main(String[] args) {
		int k=0;
		while (k<10) {
			
			System.out.println(k + ": "+ math(k) + " J: " + j);
			k++;
		}
		

	}

}
